{
  "name": "muplayer",
  "productName": "MuPlayer",
  "version": "1.0.0",
  "description": "A minimalist music player with multi-platform support",
  "main": "./out/main/index.js",
  "author": "tlyanyu",
  "github": "https://github.com/tlyanyu/MuPlayer",
  "repository": "github:tlyanyu/MuPlayer",
  "license": "AGPL-3.0",
  "license-file": "LICENSE",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "npx eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps || echo 'Warning: Some optional native modules could not be built. This is not critical.'",
    "build:web": "npm run build",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@applemusic-like-lyrics/core": "^0.1.3",
    "@applemusic-like-lyrics/lyric": "^0.2.4",
    "@applemusic-like-lyrics/vue": "^0.1.5",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^3.0.0",
    "@imsyy/color-utils": "^1.0.2",
    "@material/material-color-utilities": "^0.3.0",
    "@vueuse/core": "^12.8.2",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "electron-dl": "^4.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "file-saver": "^2.0.5",
    "font-list": "^1.6.0",
    "get-port": "^7.1.0",
    "github-markdown-css": "^5.8.1",
    "howler": "^2.2.4",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "marked": "^15.0.12",
    "music-metadata": "10.5.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "plyr": "^3.8.3",
    "vue-virt-list": "^1.6.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/multipart": "^9.2.1",
    "@fastify/static": "^8.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/howler": "^2.2.12",
    "@types/js-cookie": "^3.0.6",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.18.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "ajv": "^8.17.1",
    "crypto-js": "^4.2.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-log": "^5.4.3",
    "electron-vite": "^3.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^9.33.0",
    "fast-glob": "^3.3.3",
    "fastify": "^5.6.0",
    "naive-ui": "^2.43.1",
    "node-taglib-sharp": "^6.0.1",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^5.4.20",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-wasm": "^3.5.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "overrides": {
      "dmg-builder": "25.1.8",
      "electron-builder-squirrel-windows": "25.1.8"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "electron",
      "esbuild",
      "vue-demi"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
